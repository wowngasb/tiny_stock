{
  "name": "wowngasb/stock",
  "description": "A Tiny Framework skeleton application for rapid development",
  "keywords": [
    "framework",
    "tiny",
    "router"
  ],
  "homepage": "http://github.com/wowngasb/stock",
  "license": "MIT",
  "authors": [
    {
      "name": "Kong Lin",
      "email": "wuyou7410@gmail.com",
      "homepage": "http://github.com/wowngasb/"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-zlib": "*",
    "ext-json": "*",
    "ext-iconv": "*",
    "ext-pdo": "*",
    "ext-redis": "*",
    "ext-gd": "*",
    "ext-curl": "*",
    "wowngasb/tiny": "^0.46"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "php-mock/php-mock-phpunit": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "app\\": "app/",
      "tests\\": "tests/"
    }
  },
  "scripts": {
    "start": "php -S 0.0.0.0:8080 -t public public/index.php",
    "test": "phpunit"
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "wowngasb": {
      "type": "git",
      "url": "https://github.com/wowngasb/tiny.git"
    }
  }
}
